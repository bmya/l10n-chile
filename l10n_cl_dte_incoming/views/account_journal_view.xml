<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="analytic_account_journal_view" model="ir.ui.view">
			<field name="name">analytic_account_journal_view</field>
			<field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
			<field name="arch" type="xml">
                <!-- vista heredada -->
                <xpath expr="//notebook/page[@name='journal_entries']/group/group/field[@name='code']" position="after">
                    <field name="analytic_account_id"/>
                    <button type="object" name="assign_analytic_account"
                            string="Asignar Cuenta Analitica"
                            help="Asignar Cuenta Analitica"
                            icon="fa-check"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>